{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "adf-henry-cabrera"
		},
		"LS_AZURE_SQL_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'LS_AZURE_SQL'"
		},
		"LS_DATALAKE_HC_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'LS_DATALAKE_HC'"
		},
		"LS_AZURE_VAULT_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://kv-bigdata-train-dev.vault.azure.net/"
		},
		"LS_DATALAKE_HC_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://dlscursogrm.dfs.core.windows.net/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "adf-henry-cabrera_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"LS_AZURE_SQL_connectionString": {
						"value": "[parameters('LS_AZURE_SQL_connectionString')]"
					},
					"LS_DATALAKE_HC_accountKey": {
						"value": "[parameters('LS_DATALAKE_HC_accountKey')]"
					},
					"LS_AZURE_VAULT_properties_typeProperties_baseUrl": {
						"value": "[parameters('LS_AZURE_VAULT_properties_typeProperties_baseUrl')]"
					},
					"LS_DATALAKE_HC_properties_typeProperties_url": {
						"value": "[parameters('LS_DATALAKE_HC_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}