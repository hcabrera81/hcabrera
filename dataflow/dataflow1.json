{
	"name": "dataflow1",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "dsMoviesDB",
						"type": "DatasetReference"
					},
					"name": "ObtenerDLMovies"
				}
			],
			"sinks": [],
			"transformations": [
				{
					"name": "Filtro1910to2000"
				}
			],
			"scriptLines": [
				"source(output(",
				"          movie as string,",
				"          title as string,",
				"          genres as string,",
				"          year as string,",
				"          Rating as string,",
				"          RottenTomato as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> ObtenerDLMovies",
				"ObtenerDLMovies filter(ERROR_FUNCTION('')) ~> Filtro1910to2000"
			]
		}
	}
}